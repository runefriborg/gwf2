@target Source
:output some_file_a some_file_b

touch some_file_a
touch some_file_b

@target CheckpointedPath
:input some_file_a
:output some_file_aa
:checkpointed

cat some_file_a > some_file_aa

@target NonCheckpointedPath
:input some_file_b
:output some_file_bb

cat some_file_b > some_file_bb

@target Sink
:input some_file_aa some_file_bb
:output some_file_aa_bb

cat some_file_aa some_file_bb > some_file_aa_bb
